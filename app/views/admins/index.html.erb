<%= form_with(model:@admin,local: true) do |form| %>
  <h2>Admin Login</h2>
  <div>
    <%= form.label :Email %><br>
    <%= form.email_field :email %>
  </div>
  <div>
    <%= form.label :Password %><br>
    <%= form.text_field :password_digest%>
  </div>
  
  <div>
    <%= form.submit "Login" %>
  </div>
<% end %>